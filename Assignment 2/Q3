//ques 3 missing number
#include <iostream>
using namespace std;
int main()
{
    int b[5]= {1, 3, 5, 9, 13}, diff= b[1]-b[0];
    //in edge case only the first and the last one element is nbot checked
    for( int i=1; i<5; i++){
        if( b[i]- b[i-1] != diff && diff> b[i]-b[i-1]){
            diff= b[i]-b[i-1];
            cout<<b[i-1]-diff<<" this is missing\n";
            // cout<< b[i-1] +diff<<"= This is the number that is missing\n";
        }
        diff=b[i]-b[i-1];
    }
}
