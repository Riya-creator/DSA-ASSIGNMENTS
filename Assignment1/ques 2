#include <iostream>
#include <algorithm>
using namespace std;
int removeDup( int *num , int size){
    sort(num,num+size );
    int j=1;
    for( int i=1;i<size;i++){
        if(num[i-1]!=num[i]){
            num[j]= num[i];
            j++;
        }
    }
    return j;
}
void show(int *num , int size){
    
    cout << "Array after removing duplicates:\n";
    for (int i = 0; i < size; i++) {
        cout << num[i] << " ";
    }
    cout << endl;
};
int main(){
    int num[100], size;

    cout << "Enter number of elements: ";
    cin >> size;

    cout << "Enter elements:\n";
    for (int i = 0; i < size; i++) {
        cin >> num[i];
    }
    size= removeDup(num, size);
    show(num, size);
}
